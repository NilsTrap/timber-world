{
  "name": "timber-international",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@9.15.4",
  "scripts": {
    "dev": "turbo run dev",
    "dev:marketing": "turbo run dev --filter=@timber/marketing",
    "dev:client": "turbo run dev --filter=@timber/client-portal",
    "dev:producer": "turbo run dev --filter=@timber/producer-portal",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "type-check": "turbo run type-check",
    "clean": "turbo run clean && rm -rf node_modules"
  },
  "devDependencies": {
    "turbo": "^2.3.3",
    "typescript": "^5"
  }
}
